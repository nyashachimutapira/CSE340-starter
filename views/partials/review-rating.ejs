<% /* Simple stars display for a numeric rating */ %>
<% const stars = Number(rating) || 0; %>
<span class="rating-stars">
  <% for (let i = 1; i <= 5; i++) { %>
    <% if (i <= stars) { %>
      <span class="star filled">★</span>
    <% } else { %>
      <span class="star">☆</span>
    <% } %>
  <% } %>
</span>
